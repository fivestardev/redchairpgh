<div class="card">    
  <div class="card-content">
    <%= simple_form_for(@mentee) do |f| %>
      <%= f.error_notification %>
      <div class="row">
        <div class="form-inputs">
          
          <%# f.input :role, collection: Mentor::ROLE, include_blank: true %>
          <%= f.input :current_role, collection: Mentor::CURR_ROLE, include_blank: true %>
          <%#= f.input :impact, collection: Mentor::IMPACT, include_blank: true %>
          <%= f.input :mentor_gender, collection: Mentor::GENDER, include_blank: true %>
          <%#= f.input :comm_frequency, communication_frequency: Mentor::COMMUNICATION_FREQ, include_blank: true %>
          <%#= f.input :impact, experience: Mentor::EXPERIENCE, include_blank: true %>
          <%= f.input :years_in_field %>
          <%= f.input :lead_team %>
          <%= f.input :in_five_years, text_field_tag: { class: 'active' } %>
          <%= f.input :bio, text_field_tag: { class: 'active' } %>


      </div>
    </div>
  </div>
</div>
  
  <div class="row">
    <div class="form-actions col s7">
      <%= f.button :submit %>
    </div>
  </div>
<% end %>
